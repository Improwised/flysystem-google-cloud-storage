{
  "name": "improwised/flysystem-google-storage",
  "description": "Improwised Flysystem adapter for Google Cloud Storage",
  "license": "MIT",
  "authors": [
    {
      "name": "Improwised.com a division of Takealot Online (Pty) Ltd",
      "email": "info@improwised.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "google/cloud-storage": "~1.0",
    "league/flysystem": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Improwised\\Flysystem\\GoogleStorage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Improwised\\Flysystem\\GoogleStorage\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
